{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
          "type": "package",
          "package": {
            "name": "arthur-e/wicket",
            "version": "1.3.4",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/arthur-e/Wicket/archive/1.3.4.zip",
              "type": "zip"
            }
          }
        },
        {
            "type": "package",
            "package": {
                "name": "bootstrap/bootstrap-toggle",
                "version": "2.2.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/minhur/bootstrap-toggle/archive/2.2.2.zip",
                    "type": "zip"
                }
            }
        }

    ],
    "require": {
        "arthur-e/wicket": "^1.3",
        "bootstrap/bootstrap-toggle": "^2.2",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^1.9",
        "drupal/admin_toolbar": "^2.3",
        "drupal/backup_migrate": "^5.0",
        "drupal/barcodes": "^2.0",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/field_group": "^3.3",
        "drupal/flag": "^4.0",
        "drupal/geocoder": "^3.30",
        "drupal/geocoder_autocomplete": "^2.0",
        "drupal/geofield": "^1.1",
        "drupal/geofield_map": "^3.0",
        "drupal/leaflet": "^2.0",
        "drupal/leaflet_more_maps": "^2.1",
        "drupal/libraries": "^3.0",
        "drupal/pathauto": "^1.4",
        "drupal/responsive_favicons": "^1.5",
        "drupal/showcase_lite": "^1.1",
        "drupal/thunder_admin": "^4.0",
        "drupal/verf": "^1.0",
        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_bulk_operations": "~4.0.1",
        "drupal/views_sort_null_field": "^1.1",
        "drush/drush": "^10.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^9"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "drupal-scaffold": {
            "locations": {
                "web-root": "public_html/"
            }
        },
        "installer-paths": {
            "public_html/core": ["type:drupal-core"],
            "public_html/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "public_html/modules/contrib/{$name}": ["type:drupal-module"],
            "public_html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "public_html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
            "public_html/modules/custom/{$name}": ["type:drupal-custom-module"],
            "public_html/themes/custom/{$name}": ["type:drupal-custom-theme"]
        }
    }
}
